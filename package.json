{
  "name": "init",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api/*": {
      "target": "http://localhost:3001/"
    }
  },
  "scripts": {
    "start": "node scripts/start.js",
    "server": "nodemon src/server/index.js",
    "build": "npm run styles && node scripts/build.js",
    "styles": "sass src/styles/App.scss:public/styles/index.css --style=compressed",
    "test": "node scripts/test.js --env=jsdom",
    "redux": "NODE_ENV=production nodemon src/store/client.js --exec babel-node --presets es2015,stage-2 "
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "chalk": "1.1.3",
    "dotenv": "4.0.0",
    "express": "^4.15.4",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "jest": "20.1.0-alpha.3",
    "moment": "^2.18.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.10",
    "object-assign": "4.1.1",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-grid-gallery": "^0.4.2",
    "react-icons": "^2.2.5",
    "react-images": "^0.5.5",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "url-loader": "0.5.9",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-plugin": "2.29.0",
    "node-sass": "^4.5.3",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
